{
  "dashboard": {
    "title": "AWS Tagging Governance - Couts par Tags",
    "tags": ["aws", "tagging", "costs", "governance"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 2,
    "refresh": "5m",
    "uid": "aws-tagging-governance",
    "panels": [
      {
        "id": 1,
        "title": "Couts Totaux du Mois en Cours",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "AWS/Billing",
            "metricName": "EstimatedCharges",
            "dimensions": { "Currency": ["USD"] },
            "region": "us-east-1",
            "statistic": "Maximum",
            "period": "86400",
            "id": "billing",
            "matchExact": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 200, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Economies via AutoShutdown (estimation/mois)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "AutoShutdown",
            "metricName": "EstimatedSavings",
            "dimensions": { "Period": ["Monthly"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "savings",
            "matchExact": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 2,
            "color": { "mode": "thresholds" },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 200, "color": "orange" }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "textMode": "value_and_name"
        }
      },
      {
        "id": 8,
        "title": "Conformite des Tags (%)",
        "type": "gauge",
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "TagCompliance",
            "metricName": "CompliancePercentage",
            "dimensions": { "Scope": ["Global"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "compliance",
            "matchExact": true
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 50, "color": "orange" },
                { "value": 80, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        },
        "options": {
          "showThresholdLabels": true,
          "showThresholdMarkers": true
        }
      },
      {
        "id": 4,
        "title": "Ressources par Type",
        "type": "bargauge",
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
        "targets": [
          {
            "refId": "ec2",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "ResourceCount",
            "metricName": "ResourcesByType",
            "dimensions": { "ResourceType": ["EC2"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "ec2count",
            "matchExact": true,
            "label": "EC2"
          },
          {
            "refId": "rds",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "ResourceCount",
            "metricName": "ResourcesByType",
            "dimensions": { "ResourceType": ["RDS"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "rdscount",
            "matchExact": true,
            "label": "RDS"
          },
          {
            "refId": "s3",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "ResourceCount",
            "metricName": "ResourcesByType",
            "dimensions": { "ResourceType": ["S3"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "s3count",
            "matchExact": true,
            "label": "S3"
          },
          {
            "refId": "lambda",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "ResourceCount",
            "metricName": "ResourcesByType",
            "dimensions": { "ResourceType": ["Lambda"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "lambdacount",
            "matchExact": true,
            "label": "Lambda"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 10, "color": "yellow" },
                { "value": 50, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Couts par Equipe (Squad)",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "CostExplorer",
            "metricName": "CostBySquad",
            "dimensions": { "Squad": ["*"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "costsquad",
            "matchExact": false
          }
        ],
        "options": {
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 9,
        "title": "Couts par Centre de Couts (CostCenter)",
        "type": "barchart",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "CostExplorer",
            "metricName": "CostByCostCenter",
            "dimensions": { "CostCenter": ["*"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "costcc",
            "matchExact": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": { "mode": "palette-classic" }
          }
        },
        "options": {
          "orientation": "horizontal",
          "xTickLabelRotation": -45
        }
      },
      {
        "id": 3,
        "title": "Evolution des Couts par Equipe (30 derniers jours)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 12 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "CostExplorer",
            "metricName": "CostBySquad",
            "dimensions": { "Squad": ["*"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "costtimeseries",
            "matchExact": false
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "custom": {
              "drawStyle": "line",
              "lineWidth": 2,
              "fillOpacity": 10,
              "pointSize": 5,
              "showPoints": "auto"
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Top Services AWS les Plus Couteux",
        "type": "table",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
        "targets": [
          {
            "refId": "A",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "CostExplorer",
            "metricName": "TopCostResources",
            "dimensions": { "Service": ["*"] },
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "topcost",
            "matchExact": false
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": { "id": "byName", "options": "Maximum" },
              "properties": [
                { "id": "unit", "value": "currencyUSD" },
                { "id": "custom.width", "value": 120 }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "displayName": "Maximum",
              "desc": true
            }
          ]
        }
      },
      {
        "id": 6,
        "title": "Ressources Non Conformes (Tags Manquants)",
        "type": "stat",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
        "targets": [
          {
            "refId": "total",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "TagCompliance",
            "metricName": "NonCompliantResources",
            "dimensions": {},
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "noncompliant",
            "matchExact": false,
            "label": "Non conformes"
          },
          {
            "refId": "compliant",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "TagCompliance",
            "metricName": "CompliantResources",
            "dimensions": {},
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "compliantcount",
            "matchExact": false,
            "label": "Conformes"
          },
          {
            "refId": "totalres",
            "datasource": { "type": "cloudwatch", "uid": "cloudwatch" },
            "namespace": "TagCompliance",
            "metricName": "TotalResources",
            "dimensions": {},
            "region": "eu-west-1",
            "statistic": "Maximum",
            "period": "21600",
            "id": "totalresources",
            "matchExact": false,
            "label": "Total"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 1, "color": "orange" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        },
        "options": {
          "graphMode": "area",
          "textMode": "value_and_name",
          "colorMode": "background"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "region",
          "type": "custom",
          "label": "Region",
          "query": "eu-west-1",
          "current": {
            "text": "eu-west-1",
            "value": "eu-west-1"
          }
        }
      ]
    },
    "time": {
      "from": "now-7d",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5m", "15m", "30m", "1h", "6h", "1d"]
    }
  }
}
